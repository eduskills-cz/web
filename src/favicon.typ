#set page(width: 100pt, height: 100pt, margin: 0pt)
#set text(font: "Open Sans", weight: "bold")

#let medium-brown = rgb("#795548");
#let cloud-dancer = rgb("#f0efeb");
#rect(
  width: 180pt,
  height: 180pt,
  fill: rgb(medium-brown),
)

#place(center + horizon)[
  #text(size: 100pt, fill: cloud-dancer)[t]
]

